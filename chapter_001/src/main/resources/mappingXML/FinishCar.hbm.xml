<hibernate-mapping>
    <class name="mappingXML.FinishCar" table="finishcar">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="done" column="done"/>
        <many-to-one name="user_id" column="user_id"
                     class="mappingXML.Customer"/>

        <many-to-one name="car_id" column="car_id"
                     class="mappingXML.Car"/>
        <property name="linkToFile" column="linkToFile"/>
    </class>
</hibernate-mapping>