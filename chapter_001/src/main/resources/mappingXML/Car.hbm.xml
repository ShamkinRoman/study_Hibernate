<hibernate-mapping>
    <class name="mappingXML.Car" table="car">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="nameCar" column="nameCar"/>


        <many-to-one name="body_id" column="body_id"
                     class="mappingXML.Body"/>

        <many-to-one name="engine_id" column="engine_id"
                     class="mappingXML.Engine"/>
        <many-to-one name="kpp_id" column="kpp_id"
                     class="mappingXML.Kpp"/>
    </class>
</hibernate-mapping>
